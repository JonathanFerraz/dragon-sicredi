{
  "name": "sicredi-test",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.11.4",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.6",
    "@testing-library/react-hooks": "^7.0.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@unform/core": "^2.1.6",
    "@unform/web": "^2.1.6",
    "axios": "^0.21.1",
    "axios-mock-adapter": "^1.19.0",
    "axios-retry": "^3.1.9",
    "framer-motion": "^4.1.17",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "polished": "^4.1.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-icons": "^4.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "react-scroll": "^1.8.2",
    "react-spring": "^8.0.27",
    "styled-components": "^5.2.3",
    "typescript": "^4.2.4",
    "uuid": "^8.3.2",
    "web-vitals": "^0.2.4",
    "yup": "^0.32.8"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "CI=false && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/axios": "^0.14.0",
    "@types/enzyme": "^3.10.8",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/react-helmet": "^6.1.1",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-scroll": "^1.8.2",
    "@types/styled-components": "^5.1.7",
    "@types/uuid": "^8.3.0",
    "commitizen": "^4.2.3",
    "cz-conventional-changelog": "3.3.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import-helpers": "^1.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^4.3.8",
    "prettier": "^2.3.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog",
      "types": {
        "feat": {
          "description": "Adição de um novo recurso ",
          "title": "Funcionalidade"
        },
        "fix": {
          "description": "Correção de erros no projeto",
          "title": "Correção"
        },
        "docs": {
          "description": "Alterações na documentação",
          "title": "Documentação"
        },
        "style": {
          "description": "Alterações que não afetam o significado do código (espaço em branco, formatação, ponto-e-vírgula ausente, etc)",
          "title": "Estilo"
        },
        "refactor": {
          "description": "Uma mudança de código que não corrige um bug nem adiciona um recurso, apenas uma refatoração",
          "title": "Refatorar"
        },
        "perf": {
          "description": "Uma mudança de código que melhora o desempenho",
          "title": "Performance"
        },
        "test": {
          "description": "Adicionar testes ausentes ou corrigir testes existentes",
          "title": "Testes"
        },
        "build": {
          "description": "Mudanças que afetam o sistema de compilação ou dependências externas (escopos de exemplo: gulp, broccoli, yarn)",
          "title": "Build"
        },
        "ci": {
          "description": "Mudanças em nossos arquivos e scripts de configuração de CI (escopos de exemplo: Travis, Circle, BrowserStack, SauceLabs)",
          "title": "CI"
        },
        "chore": {
          "description": "Outras mudanças que não modificam os arquivos src ou de teste",
          "title": "Outro"
        },
        "revert": {
          "description": "Reverter a um commit anterior",
          "title": "Reverte"
        }
      }
    }
  }
}
